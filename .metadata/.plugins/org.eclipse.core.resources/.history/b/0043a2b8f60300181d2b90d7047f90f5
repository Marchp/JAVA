import java.util.Scanner;

public class Sum {
	public static void main(String []args)
	{
	    Scanner s=new Scanner(System.in);
	    int sum=0;
	    System.out.println("Enter n:"); 
	    int n=s.nextInt();

	    
	    for(int j=1;j<=n;j++)
	    {
	    	sum=sum+j;
	    }

	    
	    System.out.println("sum="+sum);
	}

}
